@model BusinessEntities.Database.slot_details
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<br />
<div class="container body-content">
    <h2>Create</h2>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <h4>Slots</h4>
            <hr />
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <div class="form-group">
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_details_id, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_details_id, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_details_id, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_details_parked_time, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_details_parked_time, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_details_parked_time, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_details_parked_end_time, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_details_parked_end_time, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_details_parked_end_time, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_details_parked_date, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_details_parked_date, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_details_parked_date, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_details_parked_vehicle_no, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_details_parked_vehicle_no, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_details_parked_vehicle_no, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_details_status, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_details_status, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_details_status, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-2">
                        @Html.LabelFor(model => model.slot_id, htmlAttributes: new { @class = "control-label" })
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-10 col-lg-10">
                        @Html.EditorFor(model => model.slot_id, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.slot_id, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-default" />
                    </div>
                </div>
            </div>
        </div>
    }
    @Html.Partial("_Footer")
</div>
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }


    @Styles.Render("~/Content/themes/base/css")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryui")
